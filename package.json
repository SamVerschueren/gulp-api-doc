    "gulp-util": "^3.0.4",
    "through2": "^0.6.3"
  },
  "devDependencies": {
    "chai": "^2.2.0",
    "concat-stream": "^1.4.7",
    "coveralls": "^2.11.2",
    "gulp": "^3.8.11",
